class Solution {
    public boolean isAnagram(String s, String t) {
        int[] alphabet = new int[256];
        for (int i = 0; i < s.length(); i++) alphabet[s.charAt(i)]++;
        for (int i = 0; i < t.length(); i++) alphabet[t.charAt(i)]--;
        for (int i : alphabet) if (i != 0) return false;
        return true;
    }
}
